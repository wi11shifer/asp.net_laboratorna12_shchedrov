@model IEnumerable<asp.net_laboratorna12_shchedrov.Models.User>

<h1>Users</h1>

<form asp-action="Create" method="post">
    <button type="submit">Create Sample Users</button>
</form>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Age</td>
            </tr>
        }
    </tbody>
</table>

<a href="/Company">Go to companies</a>
