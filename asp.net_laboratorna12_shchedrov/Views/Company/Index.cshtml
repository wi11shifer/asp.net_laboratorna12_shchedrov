@model IEnumerable<asp.net_laboratorna12_shchedrov.Models.Company>

<h1>Companies</h1>

<form asp-action="Create" method="post">
    <button type="submit">Create Sample Companies</button>
</form>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var company in Model)
        {
            <tr>
                <td>@company.Id</td>
                <td>@company.Name</td>
                <td>@company.Address</td>
            </tr>
        }
    </tbody>
</table>
